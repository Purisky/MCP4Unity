{
  "compilerOptions": {
    "target": "ES2022",
    "module":  "ESNext",
    "moduleResolution":  "node",
    "outDir": "./build",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["node_modules"]
}
